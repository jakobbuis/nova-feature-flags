{
    "name": "jakobbuis/nova-feature-flags",
    "description": "Control feature flags from Nova",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ylsideas/feature-flags": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Jakobbuis\\NovaFeatureFlags\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jakobbuis\\NovaFeatureFlags\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
